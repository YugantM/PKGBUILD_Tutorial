pkgname=add
pkgver=1.1
pkgrel=1
pkgdesc="Adds to integers, takes two arguments as input."
arch=("any")
makedepends=('python')
url="https://github.com/YugantM/PKGBUILD_Tutorial/python_add_module"
_dir="add"
source=("${_dir}"::"git+https://github.com/YugantM/python_add_module.git")
md5sums=('SKIP')

ls
echo "$pwd"
build() {
  cd "${srcdir}/${_dir}" 
  python setup.py build

}

check() {
  cd "${_dir}/python_add_module/${_dir}"
  python "test_${_dir}.py"
}

package() {
  cd "${_dir}/python_add_module"
  python setup.py install --root="$pkgdir/" --optimize=1 --skip-build
}
